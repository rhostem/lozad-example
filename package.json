{
  "name": "create-react-app-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "dev": "npm run start",
    "start": "REACT_APP_ENV=development PORT=3000 react-scripts start",
    "deploy": "npm run deploy:test",
    "deploy:test": "npm run build:test && npm run upload",
    "deploy:prod": "npm run build:prod && npm run upload",
    "build": "sh -ac '. .env.${REACT_APP_ENV}; react-scripts build'",
    "build:test": "REACT_APP_ENV=development npm run build",
    "build:prod": "REACT_APP_ENV=production npm run build",
    "upload": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "homepage": ".",
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --no-semi --single-quote --jsx-bracket-same-line --print-width 80 --trailing-comma es5 --parser flow --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-plugin-styled-components": "^1.2.0",
    "flow-bin": "^0.51.0",
    "gh-pages": "^1.0.0",
    "global": "^4.3.2",
    "husky": "^0.14.3",
    "immutable": "^3.8.1",
    "lint-staged": "^4.0.2",
    "polished": "^1.3.0",
    "prettier": "^1.5.3",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "next",
    "react-redux": "^5.0.5",
    "react-router": "3.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "next",
    "react-scripts": "1.0.10",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.15.6",
    "redux-thunk": "^2.2.0",
    "source-map-explorer": "^1.4.0",
    "styled-components": "^2.1.1",
    "unsplash-js": "^4.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@storybook/react": "^3.1.9",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.1.2"
  }
}
